{
	"extends": "react-app",
	"rules": {
		"eol-last": "off",
		"max-len": [
			"error",
			{
				"code": 120,
				"tabWidth": 2,
				"ignorePattern": "^import.*$"
			}
		],
		"no-tabs": "off",
		"unicorn/filename-case": [
			"error",
			{
				"case": "kebabCase"
			}
		],
		"quotes": "off",
		"@typescript-eslint/quotes": [
			"error",
			"double",
			{
				"avoidEscape": true
			}
		]
	}
}
